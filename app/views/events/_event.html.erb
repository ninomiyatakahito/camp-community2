<div class="card">
  <div class="card__body">
  <div class="event-images">
  <%= link_to image_tag(event.image, class: :class__img), event_path(event.id), method: :get %>
  </div>
  <%#= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype.id), method: :get %>
  <p class="card__summary">
    <%= event.concept %>
    <%= event.place %>
    <%= event.period%>
    <%= link_to " 【Eventer by#{event.user.nickname}】",class: :card__user %>
  </p>
  </div>
</div>